pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                sh 'echo revised'
            }
        }
        stage('Sonarqube') {
            environment {
                scannerHome = tool 'SonarQube'
            }
            steps {
                withSonarQubeEnv('SonarQube') {
                    sh "${scannerHome}/bin/sonar-scanner"
                }
            }
        }
    }
}
